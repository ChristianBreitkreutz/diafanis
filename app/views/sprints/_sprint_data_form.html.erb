<%= button_to "back to overview", root_path, method: :get %>
<%= form_for sprint, method: method do |f| %>
  <table>
    <tr>
      <td>iteration_id:<%= f.text_field :iteration_id %></td>
    </tr><tr>
      <td>name:<%= f.text_field :name %></td>
    </tr><tr>
      <td>description:<%= f.text_field :description %></td>
   </tr><tr>
      <td>member_data:<%= f.text_area :member_data %></td>
    </tr>
    <tr>
      <td>start_time:<%= f.datetime_select :start_time %></td>
    </tr>
    <tr>
      <td>end_time:<%= f.datetime_select :end_time %></td>
    </tr>
  </table>
<%= f.button :submit %>
<% end %>
