<%= button_to "back to overview", root_path, method: :get %>
<%= form_for sprint, method: method do |f| %>
  <table>
    <tr>
      <td><%= f.text_field :iteration_id %></td>
    </tr><tr>
      <td><%= f.text_field :name %></td>
    </tr><tr>
      <td><%= f.text_field :description %></td>
   </tr><tr>
      <td><%= f.text_area :member_data %></td>
    </tr>
    <tr>
      <td><%= f.datetime_select :start_time %></td>
    </tr>
    <tr>
      <td><%= f.datetime_select :end_time %></td>
    </tr>
  </table>
<%= f.button :submit %>
<% end %>
